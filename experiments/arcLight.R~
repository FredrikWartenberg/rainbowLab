debug at path.R#107: for (o in universe) {
    scgI <- sceneGraph()
    m = 0
    for (r in rays$chromaRays) {
        breakL = FALSE
        m = m + 1
        name = paste("r3", m, r$lambda, sep = "")
        tryCatch({
            I <- observer(r, o, nInt = 3)
        }, error = function(cond) {
            warning("observer error")
            breakL = TRUE
        })
        if (breakL) {
            break
        }
        scgI[[name]] <- I$scg
        dl <- list(I$angIn, I$angOut, I$ni, r$lambda, r$color)
        rayStats <- rbind(rayStats, dl)
    }
    n = n + 1
    name = paste("uniObj", n, sep = "")
    scgO[[name]] <- scgI
}
