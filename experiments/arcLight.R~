Browse[2]> ray
function(O=c(0,0,0),D=c(1,0,0),lambda=600,alpha="1")
{
    ray <- list('O'     =  O,
                'D'     = D/norm(D,type="2"),
                'lambda' = lambda,
                 'color' = lambda2rgb(lambda),
                 'alpha' = alpha)
    class(ray) <- "ray"
    return(ray)
}
